{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/products",
      "dest": "/Controller/itemsController.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/api/products/:productId",
      "dest": "/Controller/itemsController.js",
      "methods": ["GET", "PUT"]
    },
    {
      "src": "/(.*)",
      "dest": "/server.js",
      "methods": ["GET"]
    }
  ]
}
